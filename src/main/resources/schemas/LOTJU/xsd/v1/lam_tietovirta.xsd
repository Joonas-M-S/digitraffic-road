<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="lam">
        <xs:complexType>
            <xs:sequence>
                <xs:element type="xs:long" name="asemaId"/>
                <xs:element type="xs:dateTime" name="aika"/>
                <xs:element name="anturit">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="anturi" maxOccurs="unbounded" minOccurs="0">
                                <xs:complexType>
                                    <xs:attribute type="xs:string" name="laskennallinenAnturiId" use="required"/>
                                    <xs:attribute type="xs:float" name="arvo" use="required"/>
                                    <xs:attribute type="xs:dateTime" name="aikaikkunaAlku" use="optional"/>
                                    <xs:attribute type="xs:dateTime" name="aikaikkunaLoppu" use="optional"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>